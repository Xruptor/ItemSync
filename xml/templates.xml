		<Scripts>
			<OnEscapePressed>
				this:ClearFocus()
			</OnEscapePressed>
			<OnEnterPressed>

				if (this:GetName() == "ISyncMainFrame_QuickSearch") then
					ISync_SearchFrameName_Search:SetText("");
				elseif (this:GetName() == "ISync_SearchFrameName_Search") then
					ISyncMainFrame_QuickSearch:SetText("");
				end

				if (ItemSync) then ItemSync:Main_Refresh(); end

			</OnEnterPressed>
			<OnClick>this:SetFocus();</OnClick>
			<OnLeave></OnLeave>
			<OnEnter></OnEnter>
		</Scripts>
		<FontString inherits="ChatFontNormal"/>
	</EditBox>


